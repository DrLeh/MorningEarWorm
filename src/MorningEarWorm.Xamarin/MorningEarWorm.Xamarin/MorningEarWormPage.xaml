<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MorningEarWorm.Xamarin.MorningEarWormPage">
  <Label Text="Morning Ear Worm"></Label>
  <Label Text="{Binding Message}" VerticalOptions="Center" HorizontalOptions="Center" />
  <TextCell Text="{Binding Artist}" />
  <TextCell Text="{Binding Album}" />
  <Button Clicked="btnClick" />
  <ListView ItemsSource="{Binding Results}">
  </ListView>
</ContentPage>